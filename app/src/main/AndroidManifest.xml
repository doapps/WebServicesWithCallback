<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doapps.luis.ServicesWithCallBacks">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name="me.doapps.webservices.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="me.doapps.webservices.activity.Ingresar" />
        <activity android:name="me.doapps.webservices.activity.Listar" />
        <activity android:name="me.doapps.webservices.activity.Buscar" />
        <activity android:name="me.doapps.webservices.activity.Actualizar" />
        <activity android:name="me.doapps.webservices.activity.EliminarActivity"></activity>
    </application>

</manifest>